table.table.table-hover.table-striped.table-condensed
    thead
        tr
            th.col-md-4 Date
            th.col-md-4 Time
            th.col-md-4 Action
    tbody
        tr(ng-repeat="log in logs | orderBy:'date-time'")
            td.col-md-4 {{log['date-time']|date:'MMMM dd, yyyy'}}
            td.col-md-4 {{log['date-time']|date:'hh:mm a'}}
            td.col-md-4 {{log.action}}

div(infinite-scroll="loadLogs()", infinite-scroll-distance="3")